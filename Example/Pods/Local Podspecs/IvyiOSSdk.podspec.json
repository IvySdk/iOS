{
  "name": "IvyiOSSdk",
  "version": "8.7.0",
  "summary": "IvyiOSSdk for ios games and apps.",
  "description": "This is the IvySdk for ios platform.\n\n* Markdown format.\n* Don't worry about the indent, we strip it!",
  "homepage": "http://10.80.1.8/iossdk/IvyiOSSdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "IceStar": "691484812@qq.com"
  },
  "source": {
    "git": "http://10.80.1.8/iossdk/IvyiOSSdk.git",
    "tag": "8.7.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
    "LIBRARY_SEARCH_PATHS": "$(inherited)",
    "HEADER_SEARCH_PATHS": "$(inherited)",
    "OTHER_CFLAGS": "$(inherited)",
    "OTHER_LDFLAGS": "$(inherited)",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "VALID_ARCHS": "$(inherited)"
  },
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
    "LIBRARY_SEARCH_PATHS": "$(inherited)",
    "HEADER_SEARCH_PATHS": "$(inherited)",
    "OTHER_CFLAGS": "$(inherited)",
    "OTHER_LDFLAGS": "$(inherited)",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)"
  },
  "default_subspecs": [
    "Common",
    "Core"
  ],
  "frameworks": [
    "UIKit",
    "CoreFoundation",
    "QuartzCore",
    "AVKit",
    "AdServices"
  ],
  "subspecs": [
    {
      "name": "Common",
      "libraries": "sqlite3",
      "source_files": "IvyiOSSdk/Classes/Common/**/*",
      "public_header_files": "IvyiOSSdk/Classes/Common/**/*.h",
      "frameworks": [
        "StoreKit",
        "SystemConfiguration",
        "Security"
      ],
      "dependencies": {
        "SocketRocket": [
          "0.7.0"
        ]
      },
      "prefix_header_contents": "#import <IvyiOSSdk/NSString+Base64.h>",
      "user_target_xcconfig": {
        "LIBRARY_SEARCH_PATHS": [
          "${PODS_CONFIGURATION_BUILD_DIR}/EasyShowView"
        ],
        "HEADER_SEARCH_PATHS": [
          "${PODS_ROOT}/Headers/Public/EasyShowView"
        ]
      }
    },
    {
      "name": "Core",
      "source_files": "IvyiOSSdk/Classes/Core/**/*",
      "public_header_files": "IvyiOSSdk/Classes/Core/**/*.h*",
      "dependencies": {
        "IvyiOSSdk/Common": [

        ]
      },
      "resource_bundles": {
        "IvyiOSSdk-Core": [
          "IvyiOSSdk/Assets/Core/*.png",
          "IvyiOSSdk/Assets/Core/*.xib",
          "IvyiOSSdk/Assets/Core/LocalizedStrings/*.lproj/*"
        ]
      },
      "frameworks": [
        "CoreTelephony",
        "GameController",
        "MediaPlayer",
        "GameKit",
        "MessageUI",
        "iAd",
        "Security"
      ],
      "prefix_header_contents": "#import <IvyiOSSdk/NSString+Base64.h>"
    },
    {
      "name": "Appsflyer",
      "source_files": "IvyiOSSdk/Classes/Appsflyer/**/*",
      "public_header_files": "IvyiOSSdk/Classes/Appsflyer/**/*.h",
      "dependencies": {
        "AppsFlyerFramework": [
          "6.12.2"
        ],
        "IvyiOSSdk/Core": [

        ]
      },
      "frameworks": [
        "AdSupport",
        "iAd"
      ],
      "xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "APPSFLYER=1"
      }
    },
    {
